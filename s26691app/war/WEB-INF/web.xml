<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
<servlet-name>S26691appService</servlet-name>
<servlet-class>s26691app.server.KadryServiceImpl</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>S26691appService</servlet-name>
<url-pattern>/s26691app/s26691app</url-pattern>
</servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>S26691app.html</welcome-file>
  </welcome-file-list>

</web-app>
